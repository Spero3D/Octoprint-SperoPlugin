<head>
  <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css'>            
  <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
</head>


<div class="main-model">
  <div class="state-container">
    <div class="spero-logo">
      <svg
        width="280"
        height="45"
        viewBox="0 0 280 45"
        fill="none"
        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
      >
        <rect width="280" height="44.898" fill="url(#pattern0)" />
        <defs>
          <pattern
            id="pattern0"
            patternContentUnits="objectBoundingBox"
            width="1"
            height="1"
          >
            <use
              xlink:href="#image0_3_266"
              transform="translate(0 -0.0301959) scale(0.000841751 0.00524946)"
            />
          </pattern>
          <image
            id="image0_3_266"
            width="1188"
            height="202"
            xlink:href="data:image/png;base64,"
          />
        </defs>
      </svg>
    </div>
    <section class="state-section">
      <div class="information-states">
        <div>
              <center><select class="selected" id="queues" data-bind="options: $root.ports,optionsText: 'device',  value: $root.selectedPort, 
              optionsCaption:'Select port'" style="width:100%;">
              </select></center>
        </div>   
        <div>
          <i class="fas fa-rss" style="margin-right:8px"></i>Connection : <span data-bind="text: isShieldConnected"></span>
        </div>

        <div>
          <i class="fas fa-cog"style="margin-right:8px"></i>Motor State : <span data-bind="text: motorState"></span>
        </div>

        <div>
          <i class="	fas fa-exclamation-circle" style="margin-right:8px"></i>Current position : <span data-bind="text: bedPosition"></span>
        </div>
      </div>
      <div class="motor-move-buttons">
          <button  data-bind="event: {mousedown: function(){$root.deviceControl('backward')},mouseup:function(){$root.deviceControl('stop')}},visible:($root.isShieldConnected()=='Connected'),enable: ($root.queueState() == 'Idle' || $root.queueState() == 'Paused')&&($root.isShieldConnected()=='Connected')"    title="The table rail moves backward " >
            <i class="fas fa-long-arrow-alt-right" style="margin-right:8px">  Backward </i> 
          </button>
          <button data-bind="event: {mousedown: function(){$root.deviceControl('forward')},mouseup:function(){$root.deviceControl('stop')}},visible:($root.isShieldConnected()=='Connected'),enable: ($root.queueState() == 'Idle' || $root.queueState() == 'Paused')&&($root.isShieldConnected()=='Connected')" title="The table rail moves Forward" >
            <i class="fas fa-long-arrow-alt-left" style="margin-right:8px">  Forward </i> 
          </button>


          <button data-bind="click: function(){$root.deviceControl('eject')},visible:($root.isShieldConnected()=='Connected'),enable:($root.queueState() == 'Idle' || $root.queueState() == 'Paused')&&($root.isShieldConnected()=='Connected')" title="Cancel ejecting (it only takes effect when eject starts)" >
            <i class="fa fa-eject" style="margin-right:8px">  Eject </i> 
          </button>


          <button data-bind="click: function(){$root.deviceControl('stop')},visible:($root.isShieldConnected()=='Connected'),enable: ($root.queueState() == 'Idle' || $root.queueState() == 'Paused')&&($root.isShieldConnected()=='Connected')" title="Cancel ejecting (it only takes effect when eject starts)">
            <i class="fa fa-close" style="margin-right:8px">  Cancel </i> 
          </button>

      </div>
    </section>
  </div>






  <div class="queue-container">
    <section class ="queue-control-section" data-bind="visible: $root.queueState() == 'Idle',style: $root.queueState() == 'Idle' ? {padding:'5px'} : {}"> 
      
      <input data-bind="textInput: $root.selectedQueue()?$root.selectedQueue().name:'', event:{ blur :$root.saveToDataBase }" ></input>


      <button  data-bind="click: function() {  $root.createQueue()}" >
        <i class="fas fa-plus"></i>  
      </button>
      <select id="queues" data-bind="options: $root.queues, optionsText: 'name', value: $root.selectedQueue, 
                optionsCaption:'Select Queue', enable: $root.queueState() == 'Idle',event:{ blur :$root.checkCreateQueueEnable() }" style="width:100%;">
              
      </select>
      <button data-bind="visible: queueState() == 'Idle' , click: function() {  $root.deleteFromDatabase()},visible: $root.createQueueEnable() != 'idle'" title="Delete current queue.">
        <i class="fas fa-trash"></i>  
      </button>

      <button id="repeatButton" data-bind="click: repeatOnOff , visible: queueState() == 'Idle'" title="Repeat Queue  ">
        <i class="	fa fa-repeat"></i>  
      </button>


 

      <button data-bind="click: startQueue , visible: queueState() == 'Idle'" title="Start printing">
        <i class="	fas fa-play"></i>  
      </button>

    </section>

    <div class="queue-state-information">
      <!-- ko if: $root.queueName() != null -->
        <div>
          
          <span data-bind="text: 'Queue: '"></span>
          <span data-bind="text: queueName()" style= "font-weight: bold; " ></span>
        
        </div>
        <div>
          <span data-bind="text: 'Queue state: '"></span>
          <span data-bind="text: queueState()" style= "font-weight: bold; "></span>
        </div> 
       <!-- /ko -->

      <div class="pause-cancel-buttons">
        <button  data-bind="click: function(){$root.pauseStopQueue()}, visible:  queueState() != 'Cancelled' &&!(queueState() == 'Paused') && !(queueState() == 'Idle'&&itemState() != 'Cancelling')&& itemState()!='Cancelling'" title="Pause printing">
          <i class="	fas fa-pause"> </i> 
        </button >
          
        <button data-bind="click: function(){$root.pauseResumeQueue()},visible: (queueState() == 'Cancelled'|| queueState() == 'Paused') && (queueState() != 'Idle' && (itemState() == 'Finished' || itemState() == 'EjectFaild' || itemState() == 'Failed'  ))" title="Resume printing">
          <i class="	fas fa-play"> </i> 
        </button>
          


        <button data-bind="click: function(){$root.cancelQueue()}, visible: queueState() != 'Idle'&& itemState()!='Cancelling'" title="Cancel printing">
          <i class="	fas fa-stop"> </i> 
        </button>
       
      </div>
    



    </div>


    <div class="queue-items">
      <div id="queue_list" data-bind="foreach: currentItems, " >
        <div class='queue' style="border-bottom: 1px solid #bbb; " data-bind= 'style: { background: color}' >
          <div class='queue-row-container'>
            <div class='queue-inner-row-container'>
           
              <span><input type="radio"  data-bind=" click: function() { $root.pointer($data.index())},enable:($root.queueState()=='Paused'&& $root.itemState()=='Finished')||$root.itemState()=='eject fail'"></span>

              <span data-bind="text: $data.index()"></span>
              
              <span class=queue-name  data-bind="text: name"></span>
              

              <!-- ko if: $data.state() === "Await" -->
                  <div>
                    <span class="circle-state"> </span>
                    <span class="item-state"  data-bind="text: state"></span>
                  </div>  
              <!-- /ko -->
              <!-- ko if: $data.state() === "Printing" -->
                   
                    <span class="circle-state"style="background-color: #aa9a3c;"> </span>
                    <span class="item-state"  data-bind="text: state" > </span>&nbsp;
                    <div >
