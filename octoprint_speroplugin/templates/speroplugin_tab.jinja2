                </div>
                <span class="circle-state"style="background-color: blue;"> </span>
                <span class="item-state"  data-bind="text: state" ></span>
              <!-- /ko -->

              <!-- ko if: $data.state() === "Finished" -->
            
                <span class="circle-state"style="background-color: green;"> </span> 
                <span class="item-state"  data-bind="text: state" > </span>&nbsp;
          
              <!-- /ko -->

              <!-- ko if: $data.state() === "Paused" -->
                <span class="circle-state"style="background-color: green;"></span>
                <span class="item-state"  data-bind="text: state" ></span>&nbsp;
              
              <!-- /ko -->

              <!-- ko if: $data.state() === "Pausing" -->
                <span class="circle-state"style="background-color: green;"> </span>
                <span class="item-state"  data-bind="text: state" > </span>&nbsp;
              
              <!-- /ko -->

              <!-- ko if: $data.state() === "Cancelled" -->
                <span class="circle-state"style="background-color: red;"> </span>
                <span class="item-state"  data-bind="text: state" ></span>
              
              <!-- /ko -->


              <!-- ko if: $data.state() === "EjectFaild" -->
              
                <span class="circle-state"style="background-color: red;"></span>
                <span class="item-state"  data-bind="text: state" ></span>&nbsp;
              
              <!-- /ko -->

              <!-- ko if: $data.state() === "Failed" -->
            
                <span class="circle-state"style="background-color: red;"> </span>
                <span class="item-state"  data-bind="text: state" > </span>&nbsp;
            
              <!-- /ko -->

              <!-- ko if: $data.state() === "Cancelling" -->
            
                  <span class="circle-state"style="background-color: red;"> </span>
                  <span class="item-state"  data-bind="text: state" >  </span>&nbsp;
            
              <!-- /ko -->


              <div class=queue-item-adjust-buttons data-bind="visible:($root.queueState() == 'Idle' || $root.queueState() == 'Paused')">


                <button data-bind="click: function() { $root.queueItemDuplicate($data.index())}"><i class="	fas fa-copy"></i>  </button>

                <button data-bind="click: function() { $root.queueRemoveItem($data.index())}"><i class="	fas fa-trash"></i> </button>
                <button data-bind='click: function() { $root.queueItemUp($data.index())}'><i class="fas fa-angle-up"></i> </button>        
                <button   <span data-bind='click: function() { $root.queueItemDown($data.index())}'><i class="	fas fa-angle-down"></i> </button>
                  
                  

              </div>

            </div>
          </div>
        </div>
      </div>
    </div>

    











    



    




